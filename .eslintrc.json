{
  "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "chai": "readonly",
      "sinon": "readonly",
      "debug": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["error", {
      "ignoreRestSiblings": true
    }]
  }
}
